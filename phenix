#!/usr/bin/env php

<?php

use Phenix\App;
use Phenix\Console\Phenix;

require_once __DIR__ . '/vendor/autoload.php';

App::setLoggingChannel('file');

require_once __DIR__ . '/bootstrap/bootstrap.php';

/** @var \Phenix\Console\Phenix $phenix */
$phenix = App::make(Phenix::class);

$phenix->run();
